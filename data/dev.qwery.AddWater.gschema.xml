<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="addwater">
	<schema id="dev.qwery.AddWater" path="/dev/qwery/AddWater/">
	</schema>

	<schema id="dev.qwery.AddWater.Firefox" path="/dev/qwery/AddWater/Firefox/">
		<!-- General -->
		<key name="theme-enabled" type="b">
			<default>false</default>
		</key>
		<key name="selected-profile" type="s">
			<default>"Weed"</default>
		</key>

		<!-- Tab Positioning -->
		<key name="hide-single-tab" type="b">
			<default>false</default>
		</key>
		<key name="normal-width-tabs" type="b">
			<default>false</default>
		</key>
		<key name="swap-tab-close" type="b">
			<default>false</default>
		</key>
		<key name="bookmarks-toolbar-under-tabs" type="b">
			<default>false</default>
		</key>
		<key name="tabs-as-headerbar" type="b">
			<default>false</default>
		</key>

		<!-- Tab Behavior -->
		<key name="active-tab-contrast" type="b">
			 <default>false</default>
		</key>
		<key name="close-only-selected-tabs" type="b">
			<default>false</default>
		</key>
		<key name="symbolic-tab-icons" type="b">
			<default>false</default>
		</key>

		<!-- Styling -->
		<key name="hide-webrtc-indicator" type="b">
			<default>false</default>
		</key>

		<!-- Unstable -->
		<key name="system-icons" type="b">
			<default>false</default>
		</key>
		<key name="drag-window-headerbar-buttons" type="b">
			<default>false</default>
		</key>
	</schema>

	<schema path="/dev/qwery/AddWater/Thunderbird/" id="dev.qwery.AddWater.Thunderbird">
	</schema>
</schemalist>
